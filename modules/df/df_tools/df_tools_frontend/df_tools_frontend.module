<?php

/**
 * Implements hook_library_info_alter().
 */
function df_tools_frontend_library_info_alter(&$libraries, $extension) {
  if ($extension == 'quickedit_image') {
    $path = drupal_get_path('module', 'df_tools_frontend');
    $libraries['quickedit_image.inPlaceEditor.image']['js']['/' . $path . '/js/quickedit_image_browser.js'] = [];
  }
}
