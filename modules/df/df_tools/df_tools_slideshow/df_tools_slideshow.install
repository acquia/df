<?php

/**
 * @file
 * Contains DF Tools Slideshow installation routines.
 */

/**
 * Implements hook_install().
 */
function df_tools_slideshow_install() {
  // Enable translation for our block bundle.
  if (function_exists('df_tools_translation_enable_translation')) {
    df_tools_translation_enable_translation(['block_content' => ['slideshow']]);
  }
}
