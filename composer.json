{
    "name": "acquia/df",
    "description": "A framework for demoing enterprise-ready Drupal solutions.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drush/drush": "^9.0",
        "drupal/console": "^1.9.0",
        "acquia/lightning_dev": "dev-8.x-1.x"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "dev": {
            "type": "vcs",
            "url": "https://github.com/brantwynn/lightning-dev"
        },
        "lift": {
            "type": "vcs",
            "url": "https://github.com/acquia/lift-sdk-php"
        },
        "cohesion": {
            "type": "vcs",
            "url": "https://bitbucket.org/cohesion_dev/dx8-module.git"
        },
        "cohesion-theme": {
            "type": "vcs",
            "url": "https://bitbucket.org/cohesion_dev/dx8-theme.git"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer drupal-scaffold",
            "@composer push"
        ],
        "post-update-cmd": [
            "@composer package",
            "@composer push"
        ],
        "package": "Acquia\\DF\\Composer\\Package::execute",
        "push": "./vendor/bin/phing push",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": "rm -r -f docroot vendor modules/contrib themes/contrib"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "enable-patching": true,
        "patchLevel": {
           "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Allow exposed-form-in-block for block displays | http://drupal.org/node/2681947":
                "https://www.drupal.org/files/issues/2019-09-30/2681947-33.patch",
                "Field attachments not bubbled when rendering a saved field in Quick Edit | http://drupal.org/node/2851332":
                "https://www.drupal.org/files/issues/quickedit-render-field-attachments.patch",
                "Fatal error when setMainContent() method is not called (block module not installed) | http://drupal.org/node/2819219":
                "https://www.drupal.org/files/issues/core-fatal_error_main_content_block-2819219-2.patch",
                "Invoke hook after a site install is complete | http://drupal.org/node/2924549":
                "https://www.drupal.org/files/issues/core-post-site-install-hook.patch",
                "Dispatch events for changing content moderation states | http://drupal.org/node/2873287":
                "https://www.drupal.org/files/issues/2018-11-06/2873287-50.patch",
                "Fatal error: Call to a member function url() on null | http://drupal.org/project/drupal/issues/2565247":
                "https://www.drupal.org/files/issues/member-function-url-fix-2565247-4.patch",
                "Support third party settings for components within a section | http://drupal.org/node/2998114":
                "https://www.drupal.org/files/issues/2019-02-26/3015152-tps-5.patch",
                "Form blocks rendered inside layout builder break save | http://drupal.org/node/3045171":
                "https://www.drupal.org/files/issues/2019-11-15/drupal-layout_builder_unable_to_save-3045171-103.patch",
                "Improve block listing in Layout Builder... | https://drupal.org/node/2977587":
                "https://www.drupal.org/files/issues/2019-07-19/2977587-140-DO-NOT-TEST.patch",
                "Layout builder's \"Add Block\" sidebar menu UX improvements | http://drupal.org/node/3069446":
                "https://www.drupal.org/files/issues/2019-07-22/core-layout_builder-inline-block-ux-7.patch",
                "Blocks added by Layout Builder should have 'Display title' unchecked | http://drupal.org/node/3074435":
                "https://www.drupal.org/files/issues/2019-08-12/layout_builder_add_block-label-0.patch",
                "Overriding already overridden libraries-override requires knowledge of previous libraries-overrides | http://drupal.org/node/2642122":
                "https://www.drupal.org/files/issues/2019-08-29/2642122-107.patch",
                "Argument 2 passed... | http://drupal.org/node/2996114":
                "https://www.drupal.org/files/issues/2019-07-07/jsonapi_routes_config_catching.patch",
                "Do not create a new file entity to overwrite an existing entity | http://drupal.org/node/2241865":
                "https://www.drupal.org/files/issues/2019-03-15/2241865-63.patch",
                "Don't show the revision log message on creation of content, media, etc. | https://www.drupal.org/project/drupal/issues/3096906": "https://www.drupal.org/files/issues/2019-12-07/3096906-10.patch"
            },
            "drupal/acquia_contenthub": {
                "Errors on inserting duplicate entries into acquia_contenthub_publisher_export_tracking table| http://drupal.org/node/3075535":
                "https://www.drupal.org/files/issues/2019-11-08/acquia_contenthub-3075535-publisher_tracking_table_merge-3.patch"
            },
            "drupal/block_class": {
                "Integration with Drupal core's new Layout Builder| http://drupal.org/node/2998114":
                "https://www.drupal.org/files/issues/2019-11-30/2998114-67.patch"
            },
            "drupal/contact_storage": {
                "Conflict with other modules implementing same action plugin | http://drupal.org/node/2834490":
                "https://www.drupal.org/files/issues/contact-storage-action-namepsace-2834490-6.patch"
            },
            "drupal/custom_body_class": {
                "Remove node-type-class feature from module | http://drupal.org/node/3026806":
                "https://www.drupal.org/files/issues/2019-02-19/remove-dependency-from-node-type-class-3026806.patch",
                "Get error when revert a node. | http://drupal.org/node/2997700":
                "https://www.drupal.org/files/issues/2019-01-05/get-error-when-revert-node-2997700-3.patch"
            },
            "drupal/content_browser": {
                "Combined Content Browser patch | http://drupal.org/node/2851687":
                "https://www.drupal.org/files/issues/content-browser-2851687-2903529-combined-3.patch",
                "Error when trying to place a new block | http://drupal.org/node/3042028":
                "https://www.drupal.org/files/issues/2019-03-21/runtime_exception_when_placing_block-3042028-2_0.patch"
            },
            "drupal/default_content": {
                "Do not reimport existing entities | https://drupal.org/node/2698425":
                "https://www.drupal.org/files/issues/2018-12-11/default_content-dont-reimport-2698425-109.patch"
            },
            "drupal/default_content_deploy": {
                "Set content directory from the UI. | http://drupal.org/node/303830":
                "https://www.drupal.org/files/issues/2019-03-07/set-content-directory-from-the-ui-3038303-2-dcd-1.0-alpha6.patch"
            },
            "drupal/entity_block": {
                "Add UUID support. | http://drupal.org/node/2834377":
                "https://www.drupal.org/files/issues/2018-03-12/add-uuid-support-2834377-3.patch"
            },
            "drupal/entity_browser": {
                "Fixed the issue of Call to a member function getConfigDependencyKey() on null | http://drupal.org/node/2845037":
                "https://www.drupal.org/files/issues/2845037-4.patch",
                "Quick edit compatibility | http://drupal.org/node/2733605":
                "https://www.drupal.org/files/issues/entity-browser-quickedit-2733605.patch",
                "Combined patch | http://drupal.org/node/2825555 and http://drupal.org/node/2877751":
                "https://www.drupal.org/files/issues/2018-12-27/2877751-39_2825555-20_combined.patch"
            },
            "drupal/gdpr": {
                "Add icon to custom toolbar item | http://drupal.org/node/2972047":
                "https://www.drupal.org/files/issues/2019-12-03/add-icon-to-custom-toolbar-item-2972047-12.patch"
            },
            "drupal/lightning_core": {
              "Tests using FixtureContext fail due to Content Hub 2.0 enabled | http://drupal.org/node/3055022":
              "https://www.drupal.org/files/issues/2019-05-15/lightning_core-ch-publisher-fixture-setup-0.patch"
            },
            "drupal/message_notify": {
                "MessageNotifierBase calling assert()... | http://drupal.org/node/2966213":
                "https://www.drupal.org/files/issues/2018-04-25/message_notify-8.x-1.x-MessageNotifierBase-send-assert-0.patch"
            },
            "drupal/object_log": {
                "Change call to kprint_r() to kpr() to resolve PHP Fatal | http://drupal.org/node/2854348":
                "https://www.drupal.org/files/issues/use_kpr_to_print_logs-2854348-2.patch"
            },
            "drupal/page_manager": {
              "Custom attributes in blocks | http://drupal.org/node/2710169":
              "https://www.drupal.org/files/issues/2710169-block-attributes-24.patch"
            },
            "drupal/radix": {
              "Integrate with OffCanvas (Layout Builder) | http://drupal.org/node/3053579":
              "https://www.drupal.org/files/issues/2019-05-14/integrate-with-offcanvas-3053579-3.patch"
            },
            "drupal/video_embed_field": {
              "Youtube looping | http://drupal.org/node/2866001":
              "https://www.drupal.org/files/issues/2019-04-26/video_embed_field-youtube_loop-2866001-12.patch"
            }
        },
        "patches-ignore": {
            "drupal/lightning_layout": {
                "drupal/core": {
                    "2955065 - Customized layout does not display for node with customizations enabled when using full content view mode": "https://www.drupal.org/files/issues/2019-03-25/2955065-25.patch"
                }
            },
            "drupal/lightning_media": {
                "drupal/entity_browser": {
                    "2877751 - Inform users how many items they can add to a field that uses an entity browser": "https://www.drupal.org/files/issues/2018-06-28/2877751-39-8.x-2.x.patch"
                }
            },
            "drupal/jsonapi_schema": {
               "drupal/core": {
                 "3014277 - ResourceTypes should know about their fields": "https://www.drupal.org/files/issues/2019-06-06/3014277--resource-fields--36.patch",
                 "3061610 - Typed Data's EntityDeriver does not derive bundle-level data types when a bundle has the same name as its entity type (f.e. entity:comment:comment)": "https://www.drupal.org/files/issues/2019-06-14/3061610-12.patch"
              }
            }
        }
    },
    "require": {
        "acquia/lightning": "4.1.0",
        "acquia/content-hub-php": "2.x-dev",
        "bower-asset/imagesloaded": "4.1.1",
        "bower-asset/jquery-ui-slider-pips": "1.11.3",
