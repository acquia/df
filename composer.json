        "cohesion/dx8-theme": "5.7.5",
        "npm-asset/fontawesome-iconpicker": "1.3.0",
        "drupal/acquia_contenthub": "2.x-dev",
        "drupal/acsf": "2.62.0",
        "drupal/address": "1.7.0",
        "drupal/addtoany": "1.12.0",
